
    if (<%=@purpose_count%> == $(".accordion").length) {
        if (<%=@purpose_count%> != 3) {
            toggle = false;
            $(`<div class='d-flex justify-content-between align-items-center w-100'>
            <h1>목표 ${Number(<%=@purpose_count%>)+1}. 리워드 </h1>
            <h2 class='reward-${Number(<%=@purpose_count%>)+1}'>0P</h2>
            </div>`).appendTo(".testing-second-bottom-top-right.first");
            $(".purpose-arrow").addClass("arrow-rotate");
            $(".collapse").removeClass("show");
            $(`<%=render 'second-box'%>`).appendTo(".testing-second-top form");
            var scroll = $("#accordionExample<%=@purpose_count.to_i()+1%>").offset().top;
            $('html, body').animate({scrollTop : scroll}, 600);
        }
        else {
            $(`<%=render 'help-purpose'%>`).appendTo("body");
            <%# alertify.alert()
            .setting({
                'frameless':false,
                'title':"목표 추가 제한",
                'closableByDimmer':true,
                'closable': false,
                modal: false,
                transition:'pulse',
                'pinnable': false,
                message: '목표는 최대 3개만 추가할 수 있습니다.',
                'label':'Ok'
            }).show(); %>
        }
    }
